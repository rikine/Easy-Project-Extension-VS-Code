{
  "name": "easy-project-c--",
  "displayName": "Easy Project C++",
  "description": "Create easy C++ project",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "publisher": "Rikine",
  "icon": "r_2033336_Sgfpt.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rikine/Easy-Project-Extension-VS-Code"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:easy-project-c--.createProject",
    "onCommand:easy-project-c--.changeExecutable",
    "onCommand:easy-project-c--.changeDebugOrRelease"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easy-project-c--.createProject",
        "title": "Easy Project: Create Project"
      },
      {
        "command": "easy-project-c--.changeExecutable",
        "title": "Easy Project: Change name of Executable"
      },
      {
        "command": "easy-project-c--.changeDebugOrRelease",
        "title": "Easy Project: Set debug or release version"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}